<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequence for COD Settlement Batch -->
        <record id="sequence_cod_settlement_batch" model="ir.sequence">
            <field name="name">COD Settlement Batch</field>
            <field name="code">cod.settlement.batch</field>
            <field name="prefix">COD/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range">True</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>