<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">



        <!-- Aramex Services -->
        <record id="aramex_service_normal" model="shipping.company.service">
            <field name="company_id" ref="shipping_company_aramex"/>
            <field name="name">Aramex Normal</field>
            <field name="code">DOM</field>
            <field name="sequence">10</field>
            <field name="service_type">normal</field>
            <field name="delivery_time">3-5 Business Days</field>
            <field name="base_price">50</field>
            <field name="price_per_kg">5</field>
            <field name="cod_fee">10</field>
            <field name="insurance_rate">2</field>
            <field name="max_weight">30</field>
            <field name="active">True</field>
        </record>

        <record id="aramex_service_express" model="shipping.company.service">
            <field name="company_id" ref="shipping_company_aramex"/>
