<odoo>
    <data noupdate="1">

        <!-- Shipping Order Sequence -->
        <record id="seq_shipping_order" model="ir.sequence">
            <field name="name">Shipping Order</field>
            <field name="code">shipping.order</field>
            <field name="prefix">SH-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>