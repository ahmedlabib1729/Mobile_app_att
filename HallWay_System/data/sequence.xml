<odoo>
    <record id="seq_student_application_form" model="ir.sequence">
        <field name="name">Student Application Sequence</field>
        <field name="code">student.application</field>
        <field name="prefix">APP/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hallway_student" model="ir.sequence">
        <field name="name">Student ID Sequence</field>
        <field name="code">hallway.student</field>
        <field name="prefix">STU/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hallway_course" model="ir.sequence">
        <field name="name">Course Code Sequence</field>
        <field name="code">hallway.course</field>
        <field name="prefix">CRS/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hallway_enrollment" model="ir.sequence">
        <field name="name">Enrollment Number Sequence</field>
        <field name="code">hallway.enrollment</field>
        <field name="prefix">ENR/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hallway_certificate" model="ir.sequence">
        <field name="name">Certificate Number Sequence</field>
        <field name="code">hallway.certificate</field>
        <field name="prefix">CERT/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>