<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Multi-company Rules -->
        <record id="iw_config_company_rule" model="ir.rule">
            <field name="name">IW Config Multi-Company</field>
            <field name="model_id" ref="model_iw_config"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>

        <record id="iw_shipment_company_rule" model="ir.rule">
            <field name="name">IW Shipment Multi-Company</field>
            <field name="model_id" ref="model_iw_shipment"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>
    </data>
</odoo>