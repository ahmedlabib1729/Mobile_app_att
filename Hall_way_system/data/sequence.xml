<odoo>
 <!-- Sequence for Student Code -->
    <record id="seq_hallway_student" model="ir.sequence">
        <field name="name">Hallway Student</field>
        <field name="code">hallway.student</field>
        <field name="prefix">STU/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Application -->
    <record id="seq_student_application" model="ir.sequence">
        <field name="name">Student Application</field>
        <field name="code">student.application</field>
        <field name="prefix">APP/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>


    <record id="seq_hallway_enrollment" model="ir.sequence">
        <field name="name">Hallway Enrollment</field>
        <field name="code">hallway.enrollment</field>
        <field name="prefix">ENR/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Payment Receipt -->
    <record id="seq_enrollment_payment_receipt" model="ir.sequence">
        <field name="name">Payment Receipt</field>
        <field name="code">enrollment.payment.receipt</field>
        <field name="prefix">REC/%(year)s/</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>