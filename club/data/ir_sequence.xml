<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- تسلسل سندات القبض -->
        <record id="seq_payment_receipt" model="ir.sequence">
            <field name="name">سند قبض</field>
            <field name="code">payment.receipt</field>
            <field name="prefix">REC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- تسلسل طلبات الاشتراك -->
        <record id="seq_subscription_request" model="ir.sequence">
            <field name="name">طلب اشتراك</field>
            <field name="code">subscription.request</field>
            <field name="prefix">REQ/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>