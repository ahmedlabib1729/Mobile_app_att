<odoo>
    <data noupdate="1">

        <!-- Shipping Order Sequence -->
        <record id="seq_shipping_order" model="ir.sequence">
            <field name="name">Shipping Order</field>
            <field name="code">shipping.order</field>
            <field name="prefix">SH-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_tracking_number" model="ir.sequence">
            <field name="name">Tracking Number</field>
            <field name="code">shipping.tracking</field>
            <field name="prefix">TRK%(year)s%(month)s%(day)s</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>