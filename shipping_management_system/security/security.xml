<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="shipment_order_portal_rule" model="ir.rule">
        <field name="name">Portal: own shipments only</field>
        <field name="model_id" ref="model_shipment_order"/>
        <field name="domain_force">[('sender_id', '=', user.partner_id.id)]</field>
        <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
    </record>
</odoo>