<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Custody Request -->
        <record id="seq_custody_request" model="ir.sequence">
            <field name="name">Custody Request</field>
            <field name="code">custody.request</field>
            <field name="prefix">CR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="use_date_range">True</field>
        </record>
    </data>
</odoo>