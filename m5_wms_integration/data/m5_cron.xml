<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron job for status updates (if M5 provides webhook/API for status) -->
        <record id="ir_cron_m5_status_update" model="ir.cron">
            <field name="name">M5 WMS: Update Order Status</field>
            <field name="model_id" ref="model_m5_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_status()</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="False"/> <!-- Disabled by default -->
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>