<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- M5 WMS Category -->
        <record id="module_category_m5_wms" model="ir.module.category">
            <field name="name">M5 WMS</field>
            <field name="description">M5 Warehouse Management System Integration</field>
            <field name="sequence">50</field>
        </record>

        <!-- Groups -->
        <record id="group_m5_user" model="res.groups">
            <field name="name">M5 WMS User</field>
            <field name="category_id" ref="module_category_m5_wms"/>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <record id="group_m5_manager" model="res.groups">
            <field name="name">M5 WMS Manager</field>
            <field name="category_id" ref="module_category_m5_wms"/>
            <field name="implied_ids" eval="[(4, ref('group_m5_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>